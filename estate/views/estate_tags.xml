<odoo>
	<record id="estate_tags_tree" model="ir.ui.view">
		<field name="name">estate.property.tags.tree</field>
		<field name="model">estate.property.tags</field>
		<field name='arch' type="xml">
			<tree string="Tags" editable='bottom'>
				<field name="name"/>
			</tree>
		</field>
	</record>
	<record id='tags_property_action' model="ir.actions.act_window">
		<field name="name"> Estate Tags</field>
		<field name="res_model">estate.property.tags</field>
		<field name="view_mode">tree,form</field>
	</record>
</odoo>
